      "page": 11
    },
    {
      "level": "H1",
      "text": " 1 pound white fish fillets (such as cod or haddock)",
      "page": 11
    },
    {
      "level": "H1",
      "text": " 1 cup flour",
      "page": 11
    },
    {
      "level": "H1",
      "text": " 1 cup beer",
      "page": 11
    },
    {
      "level": "H1",
      "text": " 1 teaspoon baking powder",
      "page": 11
    },
    {
      "level": "H1",
      "text": " 1 teaspoon salt",
      "page": 11
    },
    {
      "level": "H1",
      "text": " 1/2 teaspoon pepper",
      "page": 11
    },
    {
      "level": "H1",
      "text": " 4 large potatoes, peeled and cut into fries",
      "page": 11
    },
    {
