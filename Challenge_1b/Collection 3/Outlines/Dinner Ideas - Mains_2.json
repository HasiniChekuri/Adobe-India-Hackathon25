      "page": 12
    },
    {
      "level": "H1",
      "text": " 4 lamb chops",
      "page": 13
    },
    {
      "level": "H1",
      "text": " 1 tablespoon fresh rosemary, chopped",
      "page": 13
    },
    {
      "level": "H1",
      "text": " 1/2 cup fresh mint leaves",
      "page": 13
    },
    {
      "level": "H1",
      "text": " 1/4 cup white vinegar",
      "page": 13
    },
    {
      "level": "H1",
